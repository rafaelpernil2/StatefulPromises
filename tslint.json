{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-plugin-prettier",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "prettier": true,
        "object-literal-sort-keys": false,
        "no-any": true,
        "no-console": false,
        "max-line-length": [
            true,
            180
        ],
        "ban-types": [
            false,
            [
                "Function"
            ]
        ],
        "trailing-comma": false,
        "comment-format": false,
        "eofline": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "node_modules"
        ]
    }
}